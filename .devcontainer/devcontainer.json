{
  "name": "nginx-playground",
  "service": "dev-machine",
  "dockerComposeFile": "docker-compose.yml",
  "workspaceFolder": "/usr/share/nginx/html",
  "mounts": [
      "source=${localWorkspaceFolder}/,target=/usr/share/nginx/html/,type=bind",
      "source=${localWorkspaceFolder}/.devcontainer/.nginx,target=/usr/share/nginx/.nginx/,type=bind"
  ],
  "postStartCommand": "nginx"
}